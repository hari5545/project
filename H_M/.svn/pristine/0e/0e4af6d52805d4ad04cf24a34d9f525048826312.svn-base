.tooltip {
  background: #eee;
  box-shadow: 0 0 5px #999999;
  color: #333;
  display: none;
  font-size: 12px;
  left: 200px;
  padding: 10px;
  position: absolute;
  text-align: center;
  top: 210px;
  width: 180px;
  z-index: 10;
  border-radius: 8px 8px 8px 8px;
  opacity: 1;
}
#chart { height: 350px; position: relative; left: 2px; width: 540px; }
.legend { font-size: 12px; }
rect { stroke-width: 2; }



