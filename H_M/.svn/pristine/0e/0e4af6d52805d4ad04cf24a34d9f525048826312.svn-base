@charset "UTF-8";


.tc_card {
  width: 18% !important;
}

hr {
  margin-bottom: 0;
  padding-bottom: 0.5rem;
}

.topSelect {
  max-width: 15rem;
  float: right;
  display: inline-block !important;
}

.h3 {
  display: inline-block !important;
}

.chart {
    background-color: #FFFFFF;
    border: 1px solid rgba(0,0,0,0.15);
    padding: 1rem;
    border-radius: 5px;
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);
}

.titleBar {
    padding: 0.3rem;
    background-color: #ffffff;
    border: 1px solid rgba(0,0,0,0.15);
    border-bottom: none;
    padding-left: 0.5rem;
    box-shadow: 0 1px 3px 0 rgba(0, 0, 0, 0.1), 0 1px 2px 0 rgba(0, 0, 0, 0.06);
}

@media only screen and (min-width: 1200px) {
  .container {
    max-width: 1345px;
  }
}
.h1 {
  font-size: 1.3rem;
}
.p-3 {
    padding: 0.75rem!important;
}

.title-text {
	  font-size: 1em;
    text-align: left;
    
}
.iconClass {
  float: right;
  padding-top: 0.2rem;

}
.card-body {
  -ms-flex: 1 1 auto;
  flex: 1 1 auto;
  margin: 0;
  padding: 1.5rem 1.5rem;
  position: relative;
}

.card-body + .card-body {
  border-top: 1px solid rgba(0, 40, 100, 0.12);
}

.card-body > :last-child {
  margin-bottom: 0;
}

@media print {
  .card-body {
    padding: 0;
  }
}

.mb-4, .my-4 {
    margin-bottom: 1rem !important;
}

