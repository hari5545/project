@(param: String) @import helper._


<style>
.ck-editor__editable_inline {
	min-height: 255px;
}
</style>
<div class="row-md-12">
	<div class="form-group">
		<div class="col-lg-12">
			<label style="text-align: left; padding-bottom: 5px;" for="editor"
				class="col-sm-12 control-label"><b>News Feeds:</b></label>
		</div>
		<div class="col-lg-12">
			<div class="col-sm-12">
				<textarea
					class="... ck-editor__editable ck-editor__editable_inline ... form-control"
					id="editor" name="editor" autocomplete="off"></textarea>
			</div>
		</div>
	</div>
</div>
<script>
	var myEditor;
	 ClassicEditor
     .create( document.querySelector( '#editor' ) )
     .then( editor => {
         //console.log( 'Editor was initialized', editor );
         myEditor = editor;
     } )
     .catch( error => {
         console.error( error );
     } ); 
</script>


